    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.0.0",
    "jest": "^29.3.1",
    "node": "^16.19.0",
    "sinon": "^15.0.1",
    "typescript": "^4.9.4"
  },
